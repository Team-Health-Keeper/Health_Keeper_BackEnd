# Server Configuration
PORT=3001
NODE_ENV=development

# MySQL Database Configuration
DB_HOST=localhost
DB_PORT=3306
DB_NAME=health_keeper
DB_USER=root
DB_PASSWORD=your_password

# JWT Configuration
JWT_SECRET=your_jwt_secret_key_change_this_in_production
JWT_EXPIRES_IN=1d

# Kakao OAuth Configuration
KAKAO_CLIENT_ID=your_kakao_rest_api_key
KAKAO_CLIENT_SECRET=your_kakao_client_secret
# 카카오 인증 후 리다이렉트될 백엔드 URL (백엔드가 처리 후 프론트엔드로 리다이렉트)
KAKAO_REDIRECT_URI=http://localhost:3001/api/auth/kakao/callback
# OpenID Connect 사용 여부 (true/false)
# OpenID Connect를 활성화한 경우에만 true로 설정
KAKAO_USE_OPENID=false

# Backend URL
BACKEND_URL=http://localhost:3001

# Frontend URL (React)
FRONTEND_URL=http://localhost:5173

# AI Server Configuration
# AI 서버 URL (없으면 Mock 데이터 사용)
AI_SERVER_URL=http://localhost:8000

# API Keys (if needed)
# OPENAI_API_KEY=your_openai_api_key
# OTHER_API_KEY=your_other_api_key

